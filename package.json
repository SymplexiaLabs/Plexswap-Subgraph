{
  "name": "plexswap-subgraph",
  "description": "PlexSwap Subgraph",
  "version": "1.0.2",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/symplexia/Plexswap-Subgraph.git"
  },
  "author": "Symplexia Labs",
  "license": "MIT",
  "workspaces": [
    "subgraphs/**/*"
  ],
  "scripts": {
    "codegen": "lerna run codegen",
    "build": "lerna run build",
    "format:check": "prettier --check \"*/**/*.{ts,tsx}\"",
    "format:write": "prettier --write \"*/**/*.{ts,tsx}\"",
    "lint": "eslint \"*/**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@graphprotocol/graph-cli": "^0.21.0",
    "@graphprotocol/graph-ts": "^0.20.0",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.0",
    "lerna": "^4.0.0",
    "prettier": "^2.4.0",
    "typescript": "^4.3.5"
  }
}
